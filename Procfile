cartpole-ddpg-off: just evaluate --policy off --algo ddpg --env cartpole
cartpole-ddpg-on: just evaluate --policy on --algo ddpg --env cartpole
cartpole-sac-off: just evaluate --policy off --algo sac  --env cartpole
cartpole-sac-on: just evaluate --policy on --algo sac --env cartpole
cartpole-afu-off: just evaluate --policy off --algo afu --env cartpole
cartpole-afu-on: just evaluate --policy on --algo afu --env cartpole
cartpole-afuperrin-off: just evaluate --policy off --algo afuperrin --env cartpole
cartpole-afuperrin-on: just evaluate --policy on --algo afuperrin --env cartpole

